<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.knime.workbench.repository.nodes">
       <node
            category-path="/timeseries"
            factory-class="org.knime.timeseries.node.filter.sample.OneSampleperDayNodeFactory"
            id="org.knime.timeseries.node.filter.sample.OneSampleperDayNodeFactory"/>
       <node
            category-path="/timeseries"
            factory-class="org.knime.timeseries.node.filter.extract.ExtractTimeWindowNodeFactory"
            id="org.knime.timeseries.node.filter.extract.ExtractFromToNodeFactory"/>
      <node
            category-path="/timeseries"
            factory-class="org.knime.timeseries.node.movavg.MovingAverageNodeFactory"
            id="org.knime.timeseries.node.movavg.MovingAverageNodeFactory"/>
      <node
            category-path="/timeseries"
            factory-class="org.knime.timeseries.node.stringtotimestamp.String2DateNodeFactory"
            id="org.knime.timeseries.node.stringtotimestamp.String2DateNodeFactory">
      </node>
      <node
            category-path="/timeseries"
            expert-flag="false"
            factory-class="org.knime.timeseries.node.diff.TimeDifferenceNodeFactory"
            id="org.knime.timeseries.node.diff.TimeDifferenceNodeFactory">
      </node>
      <node
            category-path="/timeseries"
            expert-flag="false"
            factory-class="org.knime.timeseries.node.aggregate.TimeAggregatorNodeFactory"
            id="org.knime.timeseries.node.aggregate.TimeAggregatorNodeFactory">
      </node>
      <node
            category-path="timeseries"
            factory-class="org.knime.timeseries.node.generator.DateGeneratorNodeFactory"
            id="org.knime.timeseries.node.generator.DateGeneratorNodeFactory">
      </node>
   </extension>

   <extension
         point="org.knime.workbench.repository.categories">
      <category
            after="meta"
            description="Time Series Utilities"
            icon="icons/category_time.png"
            level-id="timeseries"
            name="Time Series"
            path="/"/>
   </extension>
   
</plugin>
